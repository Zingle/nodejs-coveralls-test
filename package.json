  "bugs": {
    "url": "https://github.com/Zingle/nodejs-coveralls-test/issues"
  },
  "homepage": "https://github.com/Zingle/nodejs-coveralls-test#readme",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
